                    "500": {
                        "description": "Erreur serveur",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Erreur lors de l'envoi du lien de réinitialisation"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
